{
  "name": "ndex-client",
  "version": "0.3.2",
  "description": "NDEx Javascript library",
  "main": "./dist/build/browser/bundle.js",
  "scripts": {
    "build:node": "esbuild src/index.js --bundle --platform=node --outfile=./dist/node/bundle.js",
    "build:browser": "esbuild src/index.js --bundle --platform=browser --outfile=./dist/browser/bundle.js",
    "prepublish": "run-s build",
    "lint": "eslint ./src",
    "build": "run-p build:*",
    "build-prod:node": "esbuild src/index.js --minify --bundle --platform=node --outfile=./dist/node/bundle.js",
    "build-prod:browser": "esbuild src/index.js --minify --bundle --platform=browser --outfile=./dist/browser/bundle.js",
    "build-prod": "run-p build-prod:*",
    "clean": "rimraf build/*",
    "test": "nyc mocha"
  },
  "files": [
    "./dist/node/bundle.js",
    "./dist/browser/bundle.js"
  ],
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Jing Chen",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "esbuild": "^0.12.25",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^7.2.0",
    "nock": "^13.0.11",
    "npm-run-all": "^4.1.1",
    "nyc": "^15.1.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=6.3.0"
  }
}
